    <servlet-mapping>
        <servlet-name>ForgotPasswordController</servlet-name>
        <url-pattern>/forgot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneralController</servlet-name>
        <url-pattern>/general</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dashboardCMSController</servlet-name>
        <url-pattern>/cms</url-pattern>
    </servlet-mapping>
