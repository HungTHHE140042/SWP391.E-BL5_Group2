    <servlet>
        <servlet-name>ProductDetailsController</servlet-name>
        <servlet-class>controller.ProductDetailsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogController</servlet-name>
        <servlet-class>controller.BlogController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogDetailsController</servlet-name>
        <servlet-class>controller.BlogDetailsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>faqController</servlet-name>
        <servlet-class>controller.FaqController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AboutUsController</servlet-name>
        <servlet-class>controller.AboutUsController</servlet-class>
